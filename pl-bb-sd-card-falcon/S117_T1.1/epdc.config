[version]
name = CONFIG_S117_T1.1

[display]
nvm = MACRONIX_MX25U4033E
nvm_format = EPSON
controller = IT8951
temp_mode = MANUAL
instruction_code_file = "/boot/uboot/S115_T1.1/bin/Ecode.bin"
default_vcom = 4000
default_temp = 23
default_waveform = "/boot/uboot/S115_T1.1/display/waveform.wbf"

[general]
driver_board =  FALCON_I80; FALCON_SPI; 
control_system = BEAGLEBONE_BLACK
spi_port = 1
epdc_spi_port = 1
nvm_spi_port = 1
DISPLAY_SCRAMBLE_CONFIG	= 36; 36
DISPLAY_SCRAMBLE_XOFFSET = 0; 167
DISPLAY_SCRAMBLE_YOFFSET = 0

[vcom]
dac_x1				= 242;   /* first DAC register value (25% of full scale) */
dac_y1				= 1000;   /* corresponding first voltage in mV */
dac_x2				= 968;   /* second DAC register value (75% of full scale) */
dac_y2				= 3833;   /* corresponding second voltage in mV */
vgpos_mv 			= 27770;   /* VGPOS in mV */
vgneg_mv 			=-41520;   /* VGNEG in mV */
swing_ideal 	= 70000

[hv_hardware]
hv_config_vgl = -42000
hv_config_vgh =  28000
hv_config_vsh =  15000
vcom_driver = NULL;
vcom_config = IT8951; TPS65185; MAX17135 ; vcom wert setzen 
hv_driver = IT8951;
hv_config = NULL;
hv_timing = NULL;
vcom_switch = NULL;
TOFFSET_VGL_ON	=	0
TOFFSET_VSL_ON	=	0 
TOFFSET_VSH_ON	=	2
TOFFSET_VGH_ON	=	5
TOFFSET_VGH_OFF	=	0
TOFFSET_VSH_OFF	=	0
TOFFSET_VSL_OFF	=	1
TOFFSET_VGL_OFF	=	0

[register_settings]
0x0018=0x040e; Pixel Clock
0x0300=0x00c0; Frame Data Length
0x0304=0x0404; Frame Begin/End length
0x0306=0x0400; Line Data Length
0x030c=0x02c8; source driver config
0x030e=0x0006; source gate driver config
0x0360=0x0006; source  driver advance timing
0x0362=0x0004; gate driver advanced timing
0x0372=0x01d6; gate driver GDCLK config
0x0374=0x000c; gate driver GDCLK start
0x0378=0x0000; source driver dummy pixel count
